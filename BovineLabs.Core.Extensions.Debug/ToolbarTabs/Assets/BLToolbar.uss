/*    flex-direction: row;*/
/*    overflow: hidden;*/
/*    flex-shrink: 0;*/
/*    margin: 1px 2px;*/
/*    padding: 1px;*/
/*    max-width: 200px;*/
/*}*/

.unity-base-field__label {
    -unity-text-align: upper-left;
    min-width: 50px;
    width: 70px;
}

.unity-base-field__input {
    background-color: rgb(36, 36, 34);
    flex: 1 0 0;
    overflow: hidden;
    margin: 0;
    padding: 0 1px;
}

.unity-base-popup-field__text {
    margin-right: 4px;
}

.unity-toggle {
    /*margin: 0;*/
    /*height: 22px;*/
    padding: 1px;
    margin: 1px 1px 0 0;
    height: 20px;
}

.unity-enum-field__input {
    -unity-text-align: middle-left;
    flex-direction: row;
    padding: 0 3px 2px;
    border-radius: 3px;
    border-width: 1px;
    border-left-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
}

.unity-enum-field__text {
    margin-top: 1px;
    overflow: hidden;
    -unity-text-align: middle-left;
    white-space: nowrap;
    flex-grow: 1;
    color: rgb(0, 0, 0);
}

.unity-enum-field__arrow {
    width: 12px;
    height: 12px;
    align-self: center;
}

.unity-toggle__input {
    padding: 0;
}

.unity-toggle__checkmark {
    border-width: 0;
}

.unity-toggle .unity-toggle__checkmark {
    background-color: rgb(0, 0, 0);
    background-image: none;
    flex-grow: 1;
    width: 100px;
    height: 20px;
}

.unity-toggle .unity-toggle__text {
    position: absolute;
    -unity-text-align: middle-center;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

.unity-toggle:checked .unity-toggle__checkmark {
    background-color: rgb(225, 108, 6);
}

.unity-toggle:checked .unity-toggle__text {
    color: rgb(18, 2, 7);
}

.bl-popup-field__input {
    background-color: rgb(36, 36, 34);
    color: rgb(229, 220, 197);
    border-left-color: rgb(229, 220, 197);
    border-right-color: rgb(229, 220, 197);
    border-top-color: rgb(229, 220, 197);
    border-bottom-color: rgb(229, 220, 197);
}

.unity-mask-field__input {
    background-color: rgb(36, 36, 34);
    color: rgb(229, 220, 197);
    border-color: rgb(229, 220, 197);
    width: 150px;
}

.unity-base-popup-field__arrow {
    -unity-background-image-tint-color: rgb(255, 255, 255);
    width: 8px;
    height: 8px;
}

.unity-base-popup-field__text {
    margin-right: 8px;
}

.unity-collection-view {
    background-color: rgb(0, 0, 0);
    padding-bottom: 0;
    padding-top: 0;
}

.unity-scroll-view {
    background-color: rgb(0, 0, 0);
    padding-bottom: 0;
    padding-top: 0;
}

.unity-base-dropdown__checkmark {
    height: 24px;
    width: 24px;
    margin: 5px;
}

.unity-base-dropdown__item--selected:checked {
    color: rgb(18, 2, 7);
}

.unity-base-dropdown__item {
}

.bl-popup-field__text {
    color: rgb(229, 220, 197);
}

.unity-list-view__item {
    color: rgb(229, 220, 197);
}

.unity-list-view__item--selected:checked {
    background-color: rgb(225, 108, 0);
    color: rgb(18, 2, 7);
}

.unity-scroller--vertical
{
    width: var(--scroller-size);
}

.unity-scroller--horizontal
{
    height: var(--scroller-size);
}

.unity-repeat-button {
    height: 0;
    width: 0;
    border-width: 0;
    background-image: none;
}

.unity-base-slider {
    min-height: var(--scroller-size);
    min-width: var(--scroller-size);
    margin: 0;
}

.unity-base-slider--vertical {
    width: var(--scroller-size);
}

.unity-base-slider--horizontal {
    height: var(--scroller-size);
}

.unity-slider__input
{
    margin: 0;
    padding: 0;
}

.unity-scroller .unity-base-slider__tracker {
    margin: 0;
    border-width: 0;
    background-color: rgb(36, 36, 34);
}

.unity-scroller .unity-base-slider__dragger {
    margin: 0;
    border-width: 0;
    background-color: rgb(255, 119, 0);
}

.unity-scroller--vertical .unity-base-slider__dragger {
    width: var(--scroller-size);
    left: 0;
}

.unity-scroller--horizontal .unity-base-slider__dragger {
    height: var(--scroller-size);
    top: 0;
}

.bl-toolbar-contents {
    background-color: rgb(45, 45, 42);
    height: 108px;
    padding: 2px;
}

