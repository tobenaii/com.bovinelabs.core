:root {
    --scroller-size: 4px;
}

#root {
    background-color: rgb(18, 2, 7);
}

#filter {
    padding: 0;
    width: 16px;
    height: 16px;
    margin: 4px 2px;
}

#filter .unity-base-popup-field__input {
    width: auto;
    min-width: auto;
    align-items: center;
    padding: 0;
    justify-content: center;
    margin: 0;
}

#filter .unity-base-popup-field__text {
    flex-grow: 0;
    margin: 0;
}

#filter .unity-base-popup-field__arrow {
    flex-grow: 0;
    margin: 0;
    background-image: none;
}

#show {
    width: 16px;
    height: 16px;
    border-radius: 10px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    background-color: rgb(45, 45, 42);
    padding: 4px;
    border-left-color: rgba(163, 0, 0, 0.5);
    border-right-color: rgba(163, 0, 0, 0.5);
    border-top-color: rgba(163, 0, 0, 0.5);
    border-bottom-color: rgba(163, 0, 0, 0.5);
    margin: 4px;
}

#menu {
    margin-left: 4px;
    margin-right: 4px;
    margin-top: 2px;
    flex-direction: row;
    align-items: flex-end;
}

#menu Button {
    padding-left: 12px;
    padding-right: 12px;
    border-bottom-width: 0;
    margin: 0 2px;
    border-radius: 0;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    color: rgb(210, 210, 210);
    font-size: 14px;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
    white-space: nowrap;
    background-color: rgb(14, 2, 6);
}

#menu Button.bl-toolbar-highlight {
    border-radius: 3px 3px 0 0;
    background-color: rgb(45, 45, 42);
    color: rgb(255, 119, 0);
}

.bl-toolbar-tab {
    height: 24px;
    flex-direction: row;
    padding-left: 4px;
    padding-right: 4px;
}

.horizontal {
    flex-direction: row;
    justify-content: center;
}

.unity-text-element {
    color: rgb(229, 220, 197);
    font-size: 12px;
}

.unity-base-text-field__input {
    min-width: 30px;
    color: rgb(229, 220, 197);
    cursor: initial;
}

.unity-label {
    margin: 0;
    padding: 1px;
}

.bl-toolbar-group {
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-radius: 0;
    border-left-color: rgba(229, 220, 197, 0.5);
    border-right-color: rgba(229, 220, 197, 0.5);
    border-top-color: rgba(229, 220, 197, 0.5);
    border-bottom-color: rgba(229, 220, 197, 0.5);
    margin: 0 1px;
    justify-content: space-between;
    padding: 1px;
}

.pop-animation-scale-x {
    scale: 0 1;
}

.group-name {
    font-size: 9px;
    color: rgba(229, 220, 197, 0.5);
    margin: 0;
    -unity-text-align: lower-center;
    min-height: 16px;
}

.unity-button {
    height: 20px;
    background-color: rgb(18, 2, 7);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    padding: 0 6px;
    margin: 1px;
}

.unity-button:hover {
    color: rgb(225, 108, 6);
}

.unity-button:active {
    background-color: rgb(255, 119, 0);
    color: rgb(18, 2, 7);
}

